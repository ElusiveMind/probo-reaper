  "dependencies": {
    "JSONStream": "^1.0.6",
    "bluebird": "^3.0.5",
    "bytes": "^2.2.0",
    "co": "^4.6.0",
    "co-read": "^0.1.1",
    "github": "^0.2.4",
    "lodash": "^3.10.1",
    "request": "^2.65.0",
    "request-promise": "^1.0.2",
